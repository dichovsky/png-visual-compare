{
  "name": "png-visual-compare",
  "version": "0.0.1",
  "description": "",
  "main": "dist",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist ./coverage",
    "build": "npm run clean && npm run lint && npm run test && npm run tsc -- --noEmit false",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "tsc": "tsc --noEmit --pretty",
    "test": "jest",
    "license-checker": "npx license-checker --production --onlyAllow 'MIT; MIT OR X11; BSD; ISC; Apache-2.0; Unlicense'",
    "docker:build": "docker build --compress -t jest .",
    "predocker:run": "npx make-dir-cli test-results",
    "docker:run": "docker run --rm -it -v $PWD/test-results:/usr/e2e/test-results jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dichovsky/png-visual-compare.git"
  },
  "keywords": [],
  "author": "Igor Magdich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dichovsky/png-visual-compare/issues"
  },
  "homepage": "https://github.com/dichovsky/png-visual-compare#readme",
  "dependencies": {
    "jest": "27.0.6",
    "pixelmatch": "5.2.1",
    "pngjs": "6.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "@types/jest": "27.0.1",
    "@types/node": "16.7.1",
    "@types/pixelmatch": "5.2.4",
    "@types/pngjs": "6.0.1",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "eslint": "7.32.0"
  }
}
